include <iostream>
using namespace std;

int main() {
    int T;
    cin>>T;
    for(int i=0;i<T;i++){
        int a1,a2,b1,b2;
        cin>>a1>>a2>>b1>>b2;
        int ans1=a1-a2;
        int ans2=b1-b2;
        int ans3=ans1+ans2;
        if(ans3<0){
            cout<<"YES"<<endl;
            
        }
        else{
            cout<<"NO"<<endl;
        }
    }
	return 0;
	
}
