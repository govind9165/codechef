#include <bits/stdc++.h>
using namespace std;

int main() {
    int T;
    cin>>T;
    int D;
    int v1;
    int v2;
    
    for(int i=0;i<T;i++){
        cin>>D>>v1>>v2;
        int t= ceil(D*1.0/v1);
        int t2= ceil(D*1.0/v2);
        if(t>t2){
            int ans=t-t2-1;
            cout<<ans<<endl;
        }
        else{
            cout<<"-1"<<endl;
        }
       
    }
